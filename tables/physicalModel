Table "addresses" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "street" VARCHAR(50)
  "city" VARCHAR(50)
  "uf" CHAR(2)
  "zipCode" CHAR(8)
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
}

Table "users" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "name" VARCHAR(100) 
  "email" VARCHAR(50) [unique]
  "password" VARCHAR(255)
  "lastAccess" timestamp
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
  "addressId" UUID
}

Table "reservations" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "startTime" timestamp
  "endTime" timestamp
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
  "vehicleId" UUID
  "userId" UUID
  "parkingId" UUID
}

Table "parkingLott" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "name" VARCHAR(100)
  "amountPerHour" DECIMAL(10, 2)
  "capacity" INT
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
}

Table "payments" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "amount" DECIMAL(10, 2)
  "paymentDate" timestamp 
  "userId" UUID
  "reservationId" UUID
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
}

TABLE "clients" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "userId" UUID
  "planId" UUID
  "name" VARCHAR(100)
  "email" VARCHAR(50) [unique]
  "phoneNumber" VARCHAR(15)
  "cpf" CHAR(11) [unique]
  "gender" VARCHAR(20)
  "birthDate" timestamp 
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
}

TABLE "vehicles" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "clientId" UUID 
  "planId" UUID 
  "name" VARCHAR(100)
  "plate" CHAR(7)
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
}

TABLE "clientsVehicles" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "clientId" UUID
  "vehicleId" UUID
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
}

TABLE "plans" {
  "id" UUID [pk, default: `uuid_generate_v4()`]
  "amount" DECIMAL(10, 2)
  "finalDate" timestamp
  "createdAt" timestamp [default: `CURRENT_TIMESTAMP`]
  "updatedAt" timestamp
}

Ref:"addresses"."id" < "users"."addressId"

Ref:"users"."id" < "reservations"."userId"

Ref:"parkingLott"."id" < "reservations"."parkingId"

Ref: "users"."id" < "payments"."userId"

Ref:"reservations"."id" < "payments"."reservationId"

Ref: "users"."id" < "clients"."userId"

Ref: "vehicles"."id" < "reservations"."vehicleId"

Ref: "vehicles"."id" < "clientsVehicles"."vehicleId"

Ref: "clients"."id" < "clientsVehicles"."clientId"

Ref: "plans"."id" < "vehicles"."planId"



